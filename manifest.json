{
  "manifest_version": 2,

  "name": "Unsuggest",
  "description": "This extension removes all the annoying Suggested Posts on Facebook",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*"],
      "css": ["unsuggest.css"],
      "js": ["jquery.min.js", "mutation_summary.js", "unsuggest.js"]
    }
  ],
   "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
   }
}
